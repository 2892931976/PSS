<?xml version="1.0" encoding="UTF-8"?>
<Config>
	<!-- 文件测试相关设置 -->
	<FileTestConfig Path="./FileTest/" TimeInterval="1" ConnectCount="1" ParseID="1" Desc="文件测试相关设置" />
