project(UDPTest):acelib{
  sharedname=UDPTest
  dynamicflags += TEST_UDP_BUILD_DLL
  
  Source_Files{
  	./*.cpp 
  }
  macros += __LINUX__  
  
  libout = ./
  specific (prop:microsoft){
	dllout = ../../PurenessScopeServer/Windows_Bin
  } else {
	dllout = ../../PurenessScopeServer/Linux_Bin
  }
  includes += .
  includes += ../include
}
